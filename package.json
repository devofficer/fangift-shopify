    "gulp-rename": "^2.0.0",
    "gulp-run": "^1.7.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-terser": "^2.0.1",
    "gulp-watch": "^5.0.1",
    "husky": "^8.0.3",
    "js-yaml": "^3.14.1",
    "postcss": "^8.4.21",
