{
  "name": "fangift-shopify-theme",
  "version": "1.0.4",
  "description": "Fangift shopify theme project based on gulp and tailwindcss",
  "scripts": {
    "serve": "shopify theme serve /shopify & gulp watch",
    "share": "shopify theme share /shopify",
    "dev": "shopify theme dev --store fangift-6622.myshopify.com --path shopify",
    "deploy": "gulp deploy:staging",
    "deploy:prod": "gulp deploy:prod",
    "sync": "gulp sync:staging",
    "sync:prod": "gulp sync:prod",
    "build": "gulp build",
    "build:prod": "NODE_ENV=production gulp build",
    "build:css": "gulp build:css",
    "build:js": "gulp build:js",
    "open": "shopify theme open --development",
    "watch": "gulp watch",
    "prepare": "husky install"
  },
  "author": "devofficer",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@shopify/cli": "^3.47.3",
    "@shopify/theme": "^3.47.3",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "^4.0.1",
    "gulp-copy": "^4.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
